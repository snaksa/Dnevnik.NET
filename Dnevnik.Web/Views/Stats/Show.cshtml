@model Dnevnik.ViewModels.Web.StatsViewModel
@{
    if (Model.Semester == 3)
    {
        TempData["title"] = "Годишна справка";
    }
    else
    {
        TempData["title"] = "Справка - " + Model.Semester + " срок";        
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div id="page-wrapper">
    <div id="page-inner">
        @Html.Partial("_StatsPartial", @Model)
    </div>
</div>
